<!DOCTYPE html>
<html lang="pt-BR">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
  <link rel="stylesheet" href="/stylesheet/application.css">
</head>
<body>

  <%- include('../../../partials/sidebar'); %>

  <div class="m250 row containeer py-5 titleButton">
    <h1>Usuários</h1>
    <a class="lineHeight btn btn-success mr-4" href="/administrative/user/new"><i class="fa-solid fa-user-plus"></i> Adicionar novo usuário</a>
  </div>

  <div class="m250 containeer gridUser"> 
    <% users.forEach(user => { %>
      <%- include('../../../partials/userCard', {user: user}) %> 
    <% }) %>
  </div>
  
  <script src="/javascript/application.js"></script>
</body>
</html>